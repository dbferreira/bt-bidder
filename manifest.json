{
  "name": "Battrick Bidder",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This saves me time to automatically bid on players",
  "homepage_url": "http://battrick.org",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "http://battrick.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://battrick.org/*"
      ],
      "js": [
        "src/inject/inject.js",
        "js/jquery/jquery.min.js"
      ]
    }
  ]
}